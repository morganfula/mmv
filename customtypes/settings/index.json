{
  "format": "custom",
  "id": "settings",
  "label": "Settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "site_title": {
        "type": "Text",
        "config": {
          "label": "Site Title",
          "placeholder": "Montreux Media Ventures"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": ""
        }
      },
      "keywords": {
        "type": "Text",
        "config": {
          "label": "Keywords",
          "placeholder": ""
        }
      },
      "og_image": {
        "type": "Image",
        "config": {
          "label": "OG Image",
          "constraint": {
            "width": 1200,
            "height": 630
          },
          "thumbnails": []
        }
      },
      "navigation": {
        "type": "Group",
        "config": {
          "label": "Navigation",
          "repeat": true,
          "fields": {
            "url": {
              "type": "Link",
              "config": {
                "label": "URL",
                "placeholder": "",
                "select": null,
                "allowTargetBlank": true,
                "allowText": true,
                "repeat": false
              }
            }
          }
        }
      },
      "cta": {
        "type": "Link",
        "config": {
          "label": "CTA",
          "placeholder": "Call-To-Action",
          "select": null,
          "allowTargetBlank": false,
          "allowText": true,
          "repeat": false
        }
      }
    }
  }
}
