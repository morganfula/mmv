{
  "format": "custom",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Project Title"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 690,
            "height": 823
          },
          "thumbnails": []
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "",
          "options": ["Live Perfomances", "Music Videos"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "project_text": {
              "type": "SharedSlice"
            },
            "project_title": {
              "type": "SharedSlice"
            },
            "project_intro": {
              "type": "SharedSlice"
            },
            "video_block": {
              "type": "SharedSlice"
            },
            "clients_block": {
              "type": "SharedSlice"
            },
            "quote_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
